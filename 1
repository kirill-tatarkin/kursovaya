% --- Executes on mouse press over axes background.
function axes1_ButtonDownFcn(hObject, eventdata, handles)
% hObject    handle to axes1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)


% --- Executes on button press in pushbutton1.
function pushbutton1_Callback(hObject, eventdata, handles)
cla;%очистит графическое окно
x=-6:0.01:6;%интервал построения графика
fun=get(handles.edit1,'String');%вводимая функция
ezplot(fun,x), grid on %построение функции (включена сетка)
hold on %включено изображение функции


% --- Executes on button press in pushbutton2.
function pushbutton2_Callback(hObject, eventdata, handles)
a=str2double(get(handles.edit2,'String'));%левая граница
b=str2double(get(handles.edit3,'String'));%правая граница
y=get(handles.edit1,'String');%вводимая функция
f=inline(y,'x');%зададим функцию
xmin=fminbnd(y,a,b);%найдём х минимум
ymin=f(xmin);%найдём значение функции в х минимуме(минимум функции)
z=strcat('-',y);%для нахождения максимума необходимо обратить функцию
xmax=fminbnd(z,a,b);%найдём х максимум
ymax =f(xmax);%найдём значение функции в точке максимума(максимум функции)
set(handles.edit4,'string',xmin);%выведем полученные значения
set(handles.edit6,'string',xmax);
set(handles.edit7,'string',ymin);
set(handles.edit8,'string',ymax);



